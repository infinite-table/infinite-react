{
  "name": "@infinite-table/devtools",
  "private": true,
  "version": "1.0.0",
  "description": "A Chrome extension for Infinite Table",
  "scripts": {
    "watch": "rm -fr dist && parcel watch manifest.json ./src/panel.html --host localhost --config @parcel/config-webextension",
    "start": "rm -fr dist && npm run build-only && chokidar ./src  -c 'npm run build-only'",
    "build": "rm -fr dist && npm run build-only",
    "build-only": "parcel build manifest.json ./src/panel.html --no-optimize --config @parcel/config-webextension"
  },
  "dependencies": {
    "@infinite-table/infinite-react": "^6.2.9",
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/postcss": "^4.0.15",
    "chokidar-cli": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.483.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.15",
    "tw-animate-css": "^1.2.4"
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.14.2",
    "@types/chrome": "^0.0.260",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.0",
    "parcel": "^2.14.0",
    "postcss": "^8.5.3"
  },
  "author": "",
  "license": "ISC"
}
