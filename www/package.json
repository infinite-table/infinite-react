{
  "name": "www-infinite-table",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run update-versions && npm run source-build && npm run esbuild-cjs && next dev -p 3001",
    "build": "npm --prefix dataserver install && npm run update-versions && npm run generate-lib-dts && npm run esbuild-cjs && next build && next export",
    "serve-build": "http-server -p 3000 out",
    "test-compile": "node ./build/test-compiler.js",
    "source-build": "npm run --prefix=../source build",
    "generate-lib-dts": "node build/ts-compiler/copy-lib-tsc",
    "esbuild-cjs": "npm run generate-lib-dts && node build/ts-compiler/run-build.js cjs",
    "wesbuild-cjs": "npm run generate-lib-dts && node build/ts-compiler/run-build.js cjs --watch",
    "update-versions": "node build/generate-versions-info.js",
    "//": "http://localhost:3002/api/employees?_start=0&_limit=10",
    "serve-data": "node serve-data.js",
    "build-functions": "node build/functions/compile-json-server.js",
    "watch-functions": "npm run build-functions && node build/functions/compile-json-server.js --watch"
  },
  "dependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.2.3",
    "@vanilla-extract/css": "1.1.0",
    "body-parser": "^1.19.0",
    "clipboard": "^2.0.8",
    "dashify": "^2.0.0",
    "debounce": "^1.2.1",
    "express": "^4.17.1",
    "globby": "^11.0.4",
    "humanize-string": "^2.1.0",
    "json-server": "^0.16.3",
    "modern-normalize": "^1.1.0",
    "next": "11.0.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-live": "^2.2.3",
    "react-simple-code-editor": "^0.11.0",
    "serverless-http": "^2.7.0"
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@vanilla-extract/babel-plugin": "1.0.0",
    "@vanilla-extract/webpack-plugin": "1.0.3",
    "babel-plugin-preval": "^5.0.0",
    "chalk": "^4.1.1",
    "css-loader": "^6.0.0",
    "esbuild-plugin-text-replace": "^1.1.3",
    "fs-extra": "^10.0.0",
    "http-server": "^0.12.3",
    "mini-css-extract-plugin": "^2.1.0",
    "next-plugin-preval": "^1.2.1",
    "node-browserfs": "^3.0.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.3.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "yaml": "^1.10.2"
  }
}
