    "node-browserfs": "^3.0.1",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.17",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.5.0",
    "raw-loader": "^4.0.2",
    "react-sparklines": "1.7.0",
    "reading-time": "^1.2.0",
    "remark": "^12.0.1",
    "remark-external-links": "^7.0.0",
    "remark-html": "^13.0.2",
    "remark-images": "^2.0.0",
    "remark-unwrap-images": "^2.0.0",
    "retext": "^7.0.1",
    "retext-smartypants": "^4.0.0",
    "rss": "^1.2.2",
    "serve": "^14.2.0",
    "tailwindcss": "^4.0.12",
    "typescript": "5.7.2",
    "unist-util-visit": "^4.1.0",
    "yaml": "^1.10.2"
  },
  "docsRoot": {
    "source": "./content",
    "dtsOutputFile": "./src/.gen/routes.d.ts",
    "jsonOutputFile": "./src/.gen/index.json",
    "assetsOutputFolder": "./public/gen/assets",
    "assetsPublicRoot": "/gen/assets",
    "pageExtension": ".page.md"
  }
}
