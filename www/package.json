{
  "name": "www-infinite-table",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-dev": "npm run source-build && npm run generate-lib-dts && npm run dev",
    "dev": "concurrently \"npm run copy-license\" \"npm run watch-content\" \"npm run next-dev\"",
    "watch-content": "chokidar \"./content/**/*.(page|code).(md|tsx)\" -c 'npm run index-content && npm run touch-slug-pages'",
    "index-content": "npx @thedatagrid/generate-docs-meta",
    "touch-slug-pages": "npm run touch-docs-slug-page && npm run touch-blog-slug-page",
    "touch-docs-slug-page": "touch ./src/app/\\(docs\\)/docs/[...docsPages]/page.tsx",
    "touch-blog-slug-page": "touch ./src/app/blog/[...blogpost]/page.tsx",
    "next-dev": "rm -fr .next && next dev -p 3001",
    "build": "npm run copy-license && npm run index-content && npm --prefix dataserver install && npm run generate-lib-dts && npm run update-css-vars-list && npm run next-build",
    "next-build": "next build --turbopack",
    "update-css-vars-list": "node build/create-css-vars-list.js",
    "extract-props": "node build/extract-infinite-table-props.js",
    "///": "/// copying the license file and also append --- layout: NoDocs --- at the beginning of the file",
    "copy-license": "echo \"---\ntitle: End User License Agreement\n---\n$(cat ../source/LICENSE.md | sed 's/<!--.*-->//g')\" > content/eula.page.md",
    "watch-license": "npm run copy-license && chokidar ../source/LICENSE.md -c 'npm run copy-license'",
    "serve-build": "serve -p 3000 out",
    "source-build": "npm run --prefix=.. build",
    "generate-lib-dts": "echo all-fine",
    "tscw": "npm run index-content && tsc --watch --project tsconfig.json",
    "tsc": "npm run index-content && tsc --project tsconfig.json",
    "//": "http://localhost:3002/api/employees?_start=0&_limit=10",
    "kill-prev-process": "kill-port --port 3002",
    "serve:data": "npm run kill-prev-process && nodemon serve-data.js",
    "debug:data": "npm run kill-prev-process && node --inspect serve-data.js",
    "build-functions": "node build/functions/compile-json-server.js",
    "watch-functions": "npm run build-functions && node build/functions/compile-json-server.js --watch",
    "format": "prettier --write '{src,content}/**/*.{js,jsx,ts,tsx,json,scss,css,md}'",
    "lint": "next lint",
    "start": "next start",
    "install-serverless-deps": "cd functions/email-license && npm run install-deps && cd ../on-paddle-subscription && npm run install-deps"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.19.1",
    "@codesandbox/sandpack-themes": "^2.0.21",
    "@docsearch/css": "3.0.0-alpha.41",
    "@docsearch/react": "^3.5.2",
    "@gaearon/react-collapsed": "3.1.0-forked.1",
    "@headlessui/react": "^2.2.9",
    "@infinite-table/deep-map": "0.0.1",
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "14.0.2",
    "@stripe/stripe-js": "^1.22.0",
    "@tanstack/react-query": "^5.90.2",
    "acord": "^1.0.0",
    "acorn-jsx": "^5.3.2",
    "add": "^2.0.6",
    "ag-charts-react": "^12.2.0",
    "alasql": "^1.7.3",
    "body-parser": "^1.19.0",
    "body-scroll-lock": "^3.1.3",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.8",
    "copy-to-clipboard": "^3.3.1",
    "dashify": "^2.0.0",
    "date-fns": "^2.27.0",
    "debounce": "^1.2.1",
    "express": "^4.17.1",
    "framer-motion": "^11.2.12",
    "github-slugger": "^1.3.0",
    "globby": "^11.0.4",
    "hex2rgba": "^0.0.1",
    "humanize-string": "^2.1.0",
    "json-server": "^0.17.3",
    "mermaid": "^11.9.0",
    "modern-normalize": "^1.1.0",
    "next": "15.5.4",
    "parse-numeric-range": "^1.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-live": "^2.2.3",
    "react-select": "^5.4.0",
    "remark-mdx-code-meta": "^2.0.0",
    "scroll-into-view-if-needed": "^2.2.25",
    "serverless-http": "^2.7.0",
    "shadcn-ui-css-vars": "^2.0.0",
    "stripe": "^8.193.0",
    "tailwind-merge": "^3.3.1"
  },
  "overrides": {
    "next": "$next"
  },
  "devDependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/material": "latest",
    "@mui/x-date-pickers": "latest",
    "@tailwindcss/postcss": "^4.0.12",
    "@thedatagrid/generate-docs-meta": "^1.0.5",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/classnames": "^2.2.10",
    "@types/d3-dispatch": "^3.0.7",
    "@types/debounce": "^1.2.1",
    "@types/github-slugger": "^1.3.0",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^14.14.37",
