{
  "name": "@infinite-table/infinite-react",
  "description": "Infinite Table for React",
  "keywords": [
    "react",
    "infinite-table",
    "react-table",
    "table",
    "datagrid",
    "react-datagrid",
    "react-infinite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/infinite-table/infinite-react.git"
  },
  "bugs": {
    "url": "https://github.com/infinite-table/infinite-react/issues"
  },
  "version": "0.5.0",
  "main": "index.js",
  "module": "index.esm.js",
  "typings": "index.d.ts",
  "scripts": {
    "build": "rimraf dist && npm run tsc && npm run concat-dts && node ./build/buildPackageJson.js && npm run esbuild && npm run update-md && cp LICENSE.md ./dist && cp ../README.md ./dist",
    "watch": "npm run esbuild-esm && concurrently \"npm run tscw\" \"npm run esbuild-esm-watch\" \"npm run concat-dts-watch\"",
    "rollup-types": "rollup -c rollup.config.js",
    "update-md": "npm run --prefix .. doctoc",
    "esbuild": "npm run esbuild-esm && npm run esbuild-cjs",
    "esbuild-deepmap": "npm run esbuild-deepmap-esm && npm run esbuild-deepmap-cjs",
    "esbuild-deepmap-esm": "node run-build-deepmap.js esm",
    "esbuild-esm": "node run-build.js esm",
    "esbuild-esm-watch": "node run-build.js esm --watch",
    "esbuild-cjs": "node run-build.js cjs",
    "esbuild-deepmap-cjs": "node run-build-deepmap.js cjs",
    "esbuild-cjs-watch": "node run-build.js cjs --watch",
    "test": "npm run --prefix=examples test ",
    "//-": " only build declaration files",
    "tsc": "tsc --project tsconfig.build.json",
    "concat-dts": "tsup src/index.tsx --dts-only",
    "concat-dts-watch": "tsup src/index.tsx --dts-only --watch",
    "xxx - no longer used - bundle-types": "dts-bundle-generator --config bundler.config.js",
    "tscw": "tsc --watch --project tsconfig.dev.json --outDir dist",
    "xxx - no longer used - concat-dts": "node run-dts-concat.js",
    "typedoc": "typedoc",
    "lint": "tsdx lint src",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,scss,css,md}'",
    "/": "//",
    "/--- should not run locally ----": "//",
    "/--- as this is run via github actions CI ----": "//",
    "registry-publish": "npm publish ./dist --access public",
    "registry-publish-canary": "npm publish ./dist --access public --tag canary",
    "//": "// only used in the release.yml workflow",
    "release:canary-nobump": "npm run registry-publish-canary",
    "release:nobump": "npm run registry-publish",
    "///": "//",
    "bump:canary": "npm version prerelease --preid=canary",
    "bump:major:canary": "npm version premajor --preid=canary",
    "bump:minor:canary": "npm version preminor --preid=canary",
    "bump:patch:canary": "npm version prepatch --preid=canary",
    "////": "//",
    "bump:patch": "npm version patch",
    "bump:minor": "npm version minor",
    "bump:major": "npm version major"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx|ts|tsx)": [
      "echo ok",
      "git add"
    ],
    "src/**/*.(js|jsx|ts|tsx|json|scss|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.6.3",
    "@types/debug": "^4.1.7",
    "@types/jest": "^24.0.19",
    "@types/node": "^18.8.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/esbuild-plugin": "^2.2.0",
    "@vanilla-extract/recipes": "^0.2.5",
    "@vanilla-extract/sprinkles": "^1.5.0",
    "autoprefixer": "^10.4.0",
    "camelcase": "^6.0.0",
    "concurrently": "^6.2.0",
    "csstype": "^2.6.10",
    "dts-bundle-generator": "^5.9.0",
    "dts-generator": "^3.0.0",
    "esbuild": "^0.12.9",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "postcss": "^8.4.17",
    "prettier": "^2.5.0",
    "prettier-eslint": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.55.1",
    "rollup-plugin-dts": "^3.0.2",
    "ts-node": "^8.8.2",
    "tsc-prog": "^2.2.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "tsup": "^5.11.13",
    "typedoc": "^0.23.16",
    "typescript": "4.6.2"
  },
  "dependencies": {
    "binary-search": "^1.3.6",
    "debug": "^4.3.1"
  },
  "license": "Commercial & Open Source",
  "//": "will be updated at build time",
  "publishedAt": 1624970570587
}
