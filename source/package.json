{
  "name": "@infinite-table/infinite-react",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "module": "index.esm.js",
  "typings": "index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "css-compile": "sass ./src/components/InfiniteTable/index.scss:./dist/index.css",
    "css-watch": "sass ./src/components/InfiniteTable/index.scss:./dist/index.css --watch",
    "tsc": "tsc --project tsconfig.build.json --outDir dist",
    "build": "rimraf dist && npm run tsc && npm run esbuild && npm run css-compile && node ./build/buildPackageJson.js && npm run update-readme && cp LICENSE.md ./dist",
    "watch": "concurrently \"npm run css-watch\" \"npm run tscw\" \"npm run esbuild-esm-watch\"",
    "update-readme": "npm run --prefix .. doctoc && cp ../README.md ./dist/README.md",
    "esbuild": "npm run esbuild-esm && npm run esbuild-cjs",
    "esbuild-esm": "node run-build.js esm",
    "esbuild-esm-watch": "node run-build.js esm --watch",
    "esbuild-cjs": "node run-build.js cjs",
    "esbuild-cjs-watch": "node run-build.js cjs --watch",
    "test": "npm run --prefix=examples test && npm run css-compile",
    "dev": "npm run --prefix=examples dev",
    "tscw": "tsc --watch --project tsconfig.build.json --outDir dist",
    "typedoc": "typedoc",
    "lint": "tsdx lint src",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,scss,css,md}'",
    "/": "//",
    "/--- should not run locally ----": "//",
    "/--- as this is run via github actions CI ----": "//",
    "registry-publish": "npm publish dist",
    "registry-publish-canary": "npm publish --tag canary dist",
    "//": "//",
    "release:canary-nobump": "npm run registry-publish-canary",
    "release:nobump": "npm run registry-publish",
    "///": "//",
    "bump:canary": "npm version prerelease --preid=canary",
    "bump:major:canary": "npm version premajor --preid=canary",
    "bump:minor:canary": "npm version preminor --preid=canary",
    "bump:patch:canary": "npm version prepatch --preid=canary",
    "////": "//",
    "bump:patch": "npm version patch",
    "bump:minor": "npm version minor",
    "bump:major": "npm version major"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(js|jsx|ts|tsx)": [
      "echo ok",
      "git add"
    ],
    "src/**/*.(js|jsx|ts|tsx|json|scss|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.6.3",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.12.6",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "4.0.0",
    "@typescript-eslint/parser": "4.0.0",
    "camelcase": "^6.0.0",
    "concurrently": "^6.2.0",
    "csstype": "^2.6.10",
    "doctoc": "^2.0.0",
    "esbuild": "^0.12.9",
    "eslint": "7.7.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "node-sass": "^4.13.1",
    "parcel": "^1.12.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.26.2",
    "ts-node": "^8.8.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.19",
    "typescript": "4.1.5"
  },
  "dependencies": {
    "binary-search": "^1.3.6",
    "debug": "^4.3.1",
    "resize-observer-polyfill": "^1.5.1"
  },
  "license": "Commercial"
}
