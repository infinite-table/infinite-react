{
  "name": "@infinite-table/examples",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5555",
    "dev-no-output": "next dev &> /dev/null",
    "build": "rimraf .next && NODE_ENV=production next build",
    "build-local": "NODE_ENV=production next build",
    "start": "next start",
    "test": "npm run build && npm run run-test",
    "test-local": "npm run build-local && npm run run-test",
    "run-test": "concurrently -s first --kill-others \"npm run start-server\" \"npm run play:test\"",
    "start-server": "http-server -p 5555 out -c300000 -s",
    "play:test": "playwright test",
    "play:test-ui": "playwright test --ui",
    "xxxtest:debug": "PWDEBUG=console playwright test",
    "test:debug": "chokidar '**/*.spec.ts'  -c 'echo \"\n{path} was modified\n\" && npm run play:test -- --retries=0 {path} --debug'",
    "test:watch": "chokidar '**/*.spec.ts' '**/*.page.tsx' -c 'test_file_path=$(echo {path} | sed s/page.tsx/spec.ts/) && npm run play:test -- --retries=0 ${test_file_path}'",
    "ci:test": "concurrently -s first --kill-others \"npm run start-server -- -s\" \"npm run play:test\"",
    "tsc": "tsc --project tsconfig.json",
    "tscw": "tsc --watch --project tsconfig.json"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@tailwindcss/postcss": "^4.1.4",
    "@tanstack/react-query": "^5.74.7",
    "@tanstack/react-table": "^8.21.2",
    "@vanilla-extract/css": "1.15.1",
    "ag-charts-react": "^11.2.4",
    "chokidar-cli": "^3.0.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "isomorphic-fetch": "^3.0.0",
    "lucide-react": "0.503.0",
    "next": "15.3.2",
    "react": "^19.1.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.1.0",
    "sinon": "^10.0.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tinycolor2": "^1.4.2",
    "typescript": "5.7.2"
  },
  "devDependencies": {
    "@playwright/test": "1.52.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/lodash": "4.14.74",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@vanilla-extract/webpack-plugin": "^2.3.18",
    "babel-plugin-dev-expression": "^0.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^5.1.0",
    "http-server": "^14.1.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "playwright": "1.32.1",
    "postcss": "^8.5.3",
    "react-select": "^5.10.1",
    "rimraf": "^3.0.2",
    "shadcn-ui-css-vars": "^2.0.0",
    "tslib": "^2.1.0",
    "tw-animate-css": "^1.2.8"
  }
}
