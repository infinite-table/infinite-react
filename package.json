{
  "name": "infinite-table",
  "version": "0.0.0",
  "private": true,
  "description": "The most productive Data Table for React",
  "main": "index.js",
  "scripts": {
    "docs": "npm start --prefix documentation",
    "dev": "npm run --prefix source dev",
    "test": "npm run --prefix source test",
    "debug:test": "npm run --prefix source/examples play:test",
    "build-website": "npm i && npm --force --prefix source install && npm run --prefix source build && npm  --force --prefix www install && npm run --prefix www build",
    "release:canary-nobump": "npm run --prefix source release:canary-nobump",
    "release:nobump": "npm run --prefix source release:nobump",
    "doctoc": "doctoc README.md && doctoc source/LICENSE.md",
    "serve:functions": "npx netlify functions:serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinite-table/react-table.git"
  },
  "author": "InfiniteTable <admin@infinite-table.com> ",
  "bugs": {
    "url": "https://github.com/infinite-table/react-table/issues"
  },
  "homepage": "https://github.com/infinite-table/react-table#readme",
  "devDependencies": {
    "@typescript-eslint/parser": "^5.26.0",
    "doctoc": "^2.0.0",
    "prettier": "^2.6.2"
  }
}
